if (Sum == Number):
    print(" %d is a Perfect Number" %Number)
else:
    print(" %d is not a Perfect Number" %Number)

n = int(input("Enter the value : "))
full = {'a'}
full.remove('a')
full1 = {'a'}
full1.remove('a')


s = 0
for N in range(1, n-1):
    for n in range(1,N-1):
    
        if(N % n == 0):
            s = s + n
        
    if s == N:    
        full1.add(N)
print(full1)        
